<?xml version="1.0" encoding="UTF-8" ?>
<NMSMPipelineDocument Version="1.5.0">
	<TrackingOptimizationTool>
		<!--Directory used for writing results.-->
		<results_directory>CasadiSynergyDrivenTO</results_directory>
		<!--Directories for input files-->
		<tracked_quantities_directory>preprocessed</tracked_quantities_directory>
		<initial_guess_directory>ncpResults</initial_guess_directory>
		<!--Name of the input model file (.osim) to use-->
		<input_model_file>RightLegAndPelvis.osim</input_model_file>
		<!--Name of the osimx model file-->
		<input_osimx_file>RightLegAndPelvis.osimx</input_osimx_file>
		<!--Name of optimal controls setting file-->
		<optimal_control_solver_settings_file>casadiSettings.xml</optimal_control_solver_settings_file>
		<!--name, excluding file type (.sto, .mot, etc), of the trial-->
		<trial_name>kick_1</trial_name>
		<!--Synergy Driven Optimal Control problem settings-->
        <states_coordinate_list>hip_flexion_r knee_angle_r ankle_angle_r</states_coordinate_list>
        <joint_position_range_scale_factor>2</joint_position_range_scale_factor>
        <joint_velocity_range_scale_factor>1.5</joint_velocity_range_scale_factor>
        <joint_acceleration_range_scale_factor>1</joint_acceleration_range_scale_factor>
		<RCNLMuscleModel>
			<!--The data directory for fitting surrogate muscle models.-->
			<surrogate_model_data_directory>surrogateData</surrogate_model_data_directory>
			<!--Indicate the surrogate model coordinates-->
			<surrogate_model_coordinate_list>hip_flexion_r hip_adduction_r hip_rotation_r knee_angle_r ankle_angle_r subtalar_angle_r</surrogate_model_coordinate_list>
			<!--Epsilon-->
			<surrogate_model_coordinate_value_threshold>5e-3</surrogate_model_coordinate_value_threshold>
			<!--*Optional* Value to be multiplied by the optimal fiber length for maximum shortening velocity calculation -->
			<maximum_shortening_velocity_multiplier>50</maximum_shortening_velocity_multiplier>
			<use_activation_saturation>true</use_activation_saturation>
			<activation_saturation_sharpness>600</activation_saturation_sharpness>
		</RCNLMuscleModel>
		<RCNLSynergyController>
			<!--Indicate if synergy vectors should be optimized-->
			<optimize_synergy_vectors>false</optimize_synergy_vectors>
			<maximum_allowable_synergy_activation>10</maximum_allowable_synergy_activation>
			<maximum_allowable_synergy_vector_weight>2</maximum_allowable_synergy_vector_weight>
		</RCNLSynergyController>
		<RCNLCostTermSet>
			<RCNLCostTerm>
				<type>generalized_coordinate_tracking</type>
				<coordinate>hip_flexion_r</coordinate>
				<is_enabled>true</is_enabled>
				<max_allowable_error>0.1</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>generalized_coordinate_tracking</type>
				<coordinate>knee_angle_r</coordinate>
				<is_enabled>true</is_enabled>
				<max_allowable_error>0.1</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>generalized_coordinate_tracking</type>
				<coordinate>ankle_angle_r</coordinate>
				<is_enabled>true</is_enabled>
				<max_allowable_error>0.1</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>controller_tracking</type>
				<controller>1</controller>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.1</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>controller_tracking</type>
				<controller>2</controller>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.1</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>controller_tracking</type>
				<controller>3</controller>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.1</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>bflh_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>bfsh_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>gasmed_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>glmax2_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>iliacus_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>recfem_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>soleus_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>tibant_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
			<RCNLCostTerm>
				<type>muscle_activation_tracking</type>
				<muscle>vasmed_r</muscle>
				<is_enabled>false</is_enabled>
				<max_allowable_error>0.05</max_allowable_error>
			</RCNLCostTerm>
		</RCNLCostTermSet>
		<RCNLConstraintTermSet>
			<RCNLConstraintTerm>
				<type>generalized_coordinate_deviation</type>
				<coordinate>hip_flexion_r</coordinate>
				<is_enabled>true</is_enabled>
				<max_error>0.1</max_error>
				<min_error>-0.1</min_error>
			</RCNLConstraintTerm>
			<RCNLConstraintTerm>
				<type>generalized_coordinate_deviation</type>
				<coordinate>knee_angle_r</coordinate>
				<is_enabled>true</is_enabled>
				<max_error>0.1</max_error>
				<min_error>-0.1</min_error>
			</RCNLConstraintTerm>
			<RCNLConstraintTerm>
				<type>generalized_coordinate_deviation</type>
				<coordinate>ankle_angle_r</coordinate>
				<is_enabled>true</is_enabled>
				<max_error>0.1</max_error>
				<min_error>-0.1</min_error>
			</RCNLConstraintTerm>
			<RCNLConstraintTerm>
				<type>kinetic_consistency</type>
				<load>hip_flexion_r_moment</load>
				<is_enabled>true</is_enabled>
				<max_error>0.1</max_error>
				<min_error>-0.1</min_error>
			</RCNLConstraintTerm>
			<RCNLConstraintTerm>
				<type>kinetic_consistency</type>
				<load>knee_angle_r_moment</load>
				<is_enabled>true</is_enabled>
				<max_error>0.1</max_error>
				<min_error>-0.1</min_error>
			</RCNLConstraintTerm>
			<RCNLConstraintTerm>
				<type>kinetic_consistency</type>
				<load>ankle_angle_r_moment</load>
				<is_enabled>true</is_enabled>
				<max_error>0.1</max_error>
				<min_error>-0.1</min_error>
			</RCNLConstraintTerm>
		</RCNLConstraintTermSet>
	</TrackingOptimizationTool>
</NMSMPipelineDocument>
